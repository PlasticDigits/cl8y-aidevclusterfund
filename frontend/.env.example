# Contract Addresses (set after deployment)
VITE_DONATION_TRANCHE_ADDRESS=
VITE_DONATION_VAULT_ADDRESS=

# Operator Addresses (comma-separated) - these wallets can access admin dashboard
# Example: 0xabc...,0xdef...
VITE_OPERATOR_ADDRESSES=

# BSC RPC URL (optional - defaults to public RPC)
VITE_BSC_RPC_URL=https://bsc-dataseed.binance.org

# WalletConnect Project ID (optional - for WalletConnect support)
VITE_WALLETCONNECT_PROJECT_ID=

# ===========================================
# Test Mode (for local Anvil testing)
# ===========================================
# Set to "true" to enable test mode with auto-connect wallet
# When using devnet-start.sh, these are set automatically in .env.local
VITE_TEST_MODE=false

# MockUSDT address (set automatically by devnet-start.sh)
VITE_USDT_ADDRESS=
